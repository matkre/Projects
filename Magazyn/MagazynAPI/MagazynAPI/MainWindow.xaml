<Window x:Class="MagazynAPI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MagazynAPI"
        mc:Ignorable="d"
        Title="MagazynAPI" Height="450" Width="800">
    <Grid>
        <Label x:Name="loglab" Content="Login" HorizontalAlignment="Center" Margin="164,133,589,0" VerticalAlignment="Top"/>
        <Label x:Name="passlab" Content="Hasło" HorizontalAlignment="Left" Margin="163,208,0,0" VerticalAlignment="Top"/>
        <Label x:Name="menulab" Content="Logowanie do magazynu" HorizontalAlignment="Center" Margin="298,80,276,0" VerticalAlignment="Top" Width="218" FontSize="14"/>
        <Button x:Name="logbut" Content="ZALOGUJ" HorizontalAlignment="Center" Margin="308,284,335,0" VerticalAlignment="Top" Width="149" RenderTransformOrigin="0.747,0.05" Click="logbut_Click"/>
        <PasswordBox x:Name="passBox" HorizontalAlignment="Left" Margin="252,216,0,0" VerticalAlignment="Top" Width="252" Height="23"/>
        <TextBox x:Name="logBox" HorizontalAlignment="Left" Height="23" Margin="252,137,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
        <Label x:Name="labmessage" HorizontalAlignment="Left" Margin="35,371,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
